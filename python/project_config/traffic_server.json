{
  "name": "trafficserver",
  "description": "The Apache Traffic Server project, tweets, wiki, github, et. al",
  "label": "Apache Traffic Server",
  "query_pipeline": "lucidfind-default",
  "github_pipeline": "github-default",
  "jira_pipeline": "jira-default",
  "mailing_list_pipeline": "mailing-list-default",
  "twitter_pipeline": "twitter-default",
  "website_pipeline": "website-default",
  "wiki_pipeline": "wiki-default",
  "websites": [
    {
      "name": "trafficserver-website",
      "label": "Apache Traffic Server Website",
      "url": "http://trafficserver.apache.org/",
      "schedule": {
        "repeatUnit": "WEEK",
        "interval": 1
      }
    }
  ],
  "mailing_lists": [
    {
      "name": "trafficserver-users",
      "label": "Traffic Server Users",
      "mbox": "trafficserver-users",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    },
    {
      "name": "trafficserver-dev",
      "label": "Traffic Server Dev",
      "mbox": "trafficserver-dev",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    }
  ],
  "jira": {
    "name": "apache-jira-trafficserver",
    "label": "Apache Traffic Server JIRA Issues",
    "url": "https://issues.apache.org/jira/browse/TRAFFICSERVER",
      "schedule": {
        "repeatUnit": "DAY",
        "interval": 1
      }
  },
  "wikis": [
    {
      "name": "trafficserver-wiki",
      "label": "Traffic Server Wiki",
      "url": "https://cwiki.apache.org/confluence/display/TS/",
      "schedule": {
        "repeatUnit": "WEEK",
        "interval": 1
      }
    }
  ]
}
