{
  "name": "httpd",
  "description": "The Apache Httpd project, tweets, wiki, github, et. al",
  "label": "Apache Httpd",
  "query_pipeline": "lucidfind-default",
  "github_pipeline": "github-default",
  "jira_pipeline": "jira-default",
  "mailing_list_pipeline": "mailing-list-default",
  "twitter_pipeline": "twitter-default",
  "website_pipeline": "website-default",
  "wiki_pipeline": "wiki-default",
  "websites": [
    {
      "name": "httpd-website",
      "label": "Apache Httpd Website",
      "url": "http://httpd.apache.org/",
      "schedule": {
        "repeatUnit": "WEEK",
        "interval": 1
      }
    }
  ],
  "mailing_lists": [
    {
      "name": "httpd-announce",
      "label": "Httpd Announce",
      "mbox": "httpd-announce",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    },
    {
      "name": "httpd-users",
      "label": "Httpd Users",
      "mbox": "httpd-users",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    },
    {
      "name": "httpd-dev",
      "label": "Httpd Dev",
      "mbox": "httpd-dev",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
    }
  ],
  "jira": {
    "name": "apache-jira-httpd",
    "label": "Apache Httpd JIRA Issues",
    "url": "https://issues.apache.org/jira/browse/HTTPD",
      "schedule": {
        "repeatUnit": "HOUR",
        "interval": 2
      }
  }
}
